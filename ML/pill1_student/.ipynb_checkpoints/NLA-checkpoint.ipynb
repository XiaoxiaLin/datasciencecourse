    "import numpy as np\n",
    "\n",
    "for n in range(2, 51):\n",
    "    alph = np.zeros(n);\n",
    "    for j in range(1, n+1):\n",
    "    alph[j-1] = 1- 2* (j-1)/(n-1)\n",
    "    \n",
    "V = np.zeros((n,n))\n",
    "for i in range(0,n):\n",
    "    V[i,j] = alph[j]**i\n",
    "    \n",
    "KV = np.linalg.norm(V, 2)*np.linalg.norm(np.linalg.inv(V),2)\n",
    "print n, KV\n"
   ]
